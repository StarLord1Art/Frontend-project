{
  "deploy": {
    "permissions": {
      "kv": "inherit"
    },
    "exclude": ["client/node_modules/"]
  },
  "compilerOptions": {
    "lib": ["deno.unstable", "deno.ns"]
  },
  "tasks": {
    "dev": "deno run --watch --allow-net --allow-read --unstable-kv server/main.ts",
    "start": "deno run --allow-net --allow-read --unstable-kv server/main.ts"
  },
  "imports": {
    "oak": "https://deno.land/x/oak@v12.6.1/mod.ts"
  }
}